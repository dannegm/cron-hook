{
    "name": "cron-hook",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon --exec babel-node src",
        "format": "prettier . --write --log-level=warn",
        "migrate": "fn() { npx prisma migrate $1; }; fn",
        "build": "npx babel src --out-dir ./build",
        "start": "node ./build",
        "prod": "pm2 start ./build"
    },
    "dependencies": {
        "@babel/cli": "^7.24.1",
        "@babel/core": "^7.24.3",
        "@babel/node": "^7.23.9",
        "@babel/preset-env": "^7.24.3",
        "@prisma/client": "5.11.0",
        "axios": "^1.6.8",
        "babel-plugin-module-resolver": "^5.0.0",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "croner": "^8.0.1",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "mailgun-js": "^0.22.0",
        "morgan": "^1.10.0",
        "nodemon": "^3.1.0",
        "pm2": "^5.3.1",
        "prettier": "^3.2.5",
        "prisma": "^5.11.0"
    }
}
